    <string name="setting_sort">Sort by Time Again</string>
    <string name="setting_sort_description">Sort by time again to ensure correct positions for machines not in the same category. Turn off can make the loading faster.</string>
    <string name="setting_everymac">Open Directly in EveryMac</string>
    <string name="setting_everymac_description">Open the EveryMac page of the model directly instead of the specification page. Will ask for specific configuration if the model has more than one configurations.</string>
    <string name="setting_everymac_warning_content">This is effective for both main page and search result. Random access will no longer available as well.</string>
    <string name="setting_deathsound">Enable Death Sound</string>
    <string name="setting_deathsound_deathsound_description">Play startup and death sound alternatively in the specification page, or play the startup sound only.</string>
    <string name="setting_high_vol_warning">High Volume Warning</string>
    <string name="setting_high_vol_warning_description">Enable high volume warning when using headphones in the specification page.</string>
    <string name="setting_gestures">Enable Navigation Gestures</string>
    <string name="setting_gestures_description">Enable left and right swipes to navigate between models in the specification page.</string>
    <string name="setting_navbuttons">Enable Navigation Buttons</string>
    <string name="setting_navbuttons_description">Enable buttons to navigate between models in the specification page.</string>
    <string name="setting_quicknav">Quick Navigation</string>
    <string name="setting_quicknav_description">Use an alternative method to make navigation faster between machines in the specification page.</string>
    <string name="setting_randomall">Limit Random Range</string>
    <string name="setting_randomall_description">Only pick random machines inside the current group instead of all categories.</string>
    <string name="setting_save_main">Save Usage in Main Page</string>
    <string name="setting_save_main_description">Save the last selected viewing method and category filter.</string>
    <string name="setting_save_search">Save Usage in Search Page</string>
    <string name="setting_save_search_description">Save the last used search keyword and filter.</string>
    <string name="setting_defaults_warning_content">Your saved preference file will be completely cleared, including your preferences, usage records, favourites, and comments. After you confirm, MacIndex will quit. This is helpful for troubleshooting, but cannot be undone.</string>
